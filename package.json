{
  "name": "nordic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "init": "cp .env.production.template .env.production && cp .env.development.template .env.development && cp .env.local.template .env.local",
    "dev": "next dev",
    "dev:server": "node ./json-server/index.js",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "lint": "lint-staged",
    "eslinttest": "eslint -c eslintrc.config.js",
    "prettiertest": "prettier . --check --config prettier.config.js",
    "prettiertest:fix": "prettier . --write --config prettier.config.js",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "concurrently \"npm run dev:server\" \"npm run dev\" \"cypress open\""
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.4",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/themes": "^2.0.3",
    "@reduxjs/toolkit": "^2.2.1",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.1",
    "negotiator": "^0.6.3",
    "next": "^14.2.3",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.0",
    "react-redux": "^9.1.0",
    "redux-saga": "^1.3.0",
    "reselect": "^5.1.0",
    "server-only": "^0.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@emotion/react": "^11.11.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/jest": "^29.5.12",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.6.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vanilla-extract/next-plugin": "^2.4.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.8.1",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
